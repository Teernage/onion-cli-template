{
  "name": "onion-newtab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "eslint": "eslint src --ext .js,.vue,.ts --fix --cache",
    "lint:prettier": "prettier --write \"src/**/*.{ts,json,css,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "commit-msg": "commitlint --config ./commitlint/.commitlintrc.cjs --edit --format ./commitlint/formatter.cjs",
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky install",
    "check": "vue-tsc --noEmit",
    "chrome": "pnpm run build && node scripts/build-and-log-crx.js"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "chalk": "^4.1.2",
    "crx": "^5.0.1",
    "crx3": "^1.1.3",
    "element-plus": "^2.7.8",
    "fs-extra": "^11.2.0",
    "pinia": "^2.2.0",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.0",
    "vue-router": "^4.4.0",
    "yauzl": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@crxjs/vite-plugin": "2.0.0-beta.23",
    "@rollup/plugin-json": "^6.1.0",
    "@testing-library/vue": "^8.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/chrome": "^0.0.287",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "axios-mock-adapter": "^2.0.0",
    "chalk": "4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "form-data": "^4.0.2",
    "happy-dom": "^17.4.4",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.10",
    "postcss-selector-parser": "^6.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "5.0.11",
    "vitest": "^3.1.2",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "configFile": "./commitlint/.commitlintrc.cjs"
  }
}
